env: "development"
log_level: "debug"
port: 9000
dynamodb:
  region: "$dev_dynamodb_region"
  task_table: "$dev_dynamodb_task_table"
  group_table: "$dev_dynamodb_group_table"
  aws_region: "$dev_dynamodb_region"
  aws_access_key: "$dev_dynamodb_aws_access_key"
  aws_secret_access: "$dev_dynamodb_aws_secret_access"
machinery:
  broker_namespace: "$dev_machinery_broker_namespace"
  broker_host: "$dev_machinery_broker_host"
  result_expiry: $dev_machinery_result_expiry # seconds