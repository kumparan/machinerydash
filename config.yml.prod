env: "production"
log_level: "debug"
port: 9000
dynamodb:
  host: "$prod_dynamodb_host"
  region: "$prod_dynamodb_region"
  is_local: "$prod_dynamodb_is_local"
  task_table: "$prod_dynamodb_task_table"
  group_table: "$prod_dynamodb_group_table"
machinery:
  broker_namespace: "$prod_machinery_broker_namespace"
  broker_host: "$prod_machinery_broker_host"
  result_expiry: $prod_machinery_result_expiry # seconds